<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fundocompletopng"
    tools:context=".ActivityConsultaCliente">

    <TextView
        android:id="@+id/textViewClientSearchScreen"
        android:layout_width="168dp"
        android:layout_height="96dp"
        android:shadowColor="#000000"
        android:shadowDx="6"
        android:shadowDy="6"
        android:shadowRadius="1"
        android:text="Cadastro Cliente"
        android:textColor="#FFFFFF"
        android:textIsSelectable="false"
        android:textSize="40sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.047" />

    <Button
        android:id="@+id/btnCancelConsultaCliente"
        android:layout_width="126dp"
        android:layout_height="47dp"
        android:background="@drawable/button_beauty_full"
        android:shadowColor="#000000"
        android:shadowDx="2"
        android:shadowDy="2"
        android:shadowRadius="1"
        android:text="Cancelar"
        android:textSize="14sp"
        android:textStyle="bold"
        app:backgroundTint="#010127"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.182"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.847" />

    <EditText
        android:id="@+id/editTextConsultaBairroCliente"
        android:layout_width="160dp"
        android:layout_height="40dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:background="@drawable/text_beauty_full"
        android:ems="10"
        android:hint="Bairro"
        android:inputType="textPersonName"
        android:padding="6sp"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.452"
        app:layout_constraintStart_toEndOf="@+id/editTextNameClient"
        app:layout_constraintTop_toBottomOf="@+id/editTextConsultaCidadeCliente"
        app:layout_constraintVertical_bias="0.027" />

    <EditText
        android:id="@+id/editTextConsultaCepCliente"
        android:layout_width="160dp"
        android:layout_height="40dp"
        android:layout_marginBottom="208dp"
        android:background="@drawable/text_beauty_full"
        android:ems="10"
        android:hint="CEP"
        android:inputType="textPersonName"
        android:padding="6sp"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextConsultaCpfCliente"
        app:layout_constraintVertical_bias="0.357" />

    <EditText
        android:id="@+id/editTextConsultaEnderecoCliente"
        android:layout_width="160dp"
        android:layout_height="40dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:background="@drawable/text_beauty_full"
        android:ems="10"
        android:hint="EndereÃ§o"
        android:inputType="textPersonName"
        android:padding="6sp"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.452"
        app:layout_constraintStart_toEndOf="@+id/editTextNameClient"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.408" />

    <EditText
        android:id="@+id/editTextConsultaEmailCliente"
        android:layout_width="160dp"
        android:layout_height="40dp"
        android:layout_marginBottom="288dp"
        android:background="@drawable/text_beauty_full"
        android:ems="10"
        android:hint="Email"
        android:inputType="textPersonName"
        android:padding="6sp"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextConsultaTelClient"
        app:layout_constraintVertical_bias="0.29" />

    <EditText
        android:id="@+id/editTextConsultaCidadeCliente"
        android:layout_width="160dp"
        android:layout_height="40dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:background="@drawable/text_beauty_full"
        android:ems="10"
        android:hint="Cidade"
        android:inputType="textPersonName"
        android:padding="6sp"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.452"
        app:layout_constraintStart_toEndOf="@+id/editTextNameClient"
        app:layout_constraintTop_toBottomOf="@+id/editTextConsultaEstadoCliente"
        app:layout_constraintVertical_bias="0.027" />

    <EditText
        android:id="@+id/editTextConsultaComplementoCliente"
        android:layout_width="160dp"
        android:layout_height="40dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:background="@drawable/text_beauty_full"
        android:ems="10"
        android:hint="Complemento"
        android:inputType="textPersonName"
        android:padding="6sp"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.452"
        app:layout_constraintStart_toEndOf="@+id/editTextNameClient"
        app:layout_constraintTop_toBottomOf="@+id/editTextConsultaBairroCliente"
        app:layout_constraintVertical_bias="0.027" />

    <EditText
        android:id="@+id/editTextConsultaCpfCliente"
        android:layout_width="160dp"
        android:layout_height="40dp"
        android:layout_marginBottom="256dp"
        android:background="@drawable/text_beauty_full"
        android:ems="10"
        android:hint="CPF/CNPJ"
        android:inputType="textPersonName"
        android:padding="6sp"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextConsultaEmailCliente" />

    <EditText
        android:id="@+id/editTextConsultaTelClient"
        android:layout_width="160dp"
        android:layout_height="40dp"
        android:layout_marginBottom="328dp"
        android:background="@drawable/text_beauty_full"
        android:ems="10"
        android:hint="Telefone"
        android:inputType="textPersonName"
        android:padding="6sp"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextConsultaNomeCliente"
        app:layout_constraintVertical_bias="0.243" />

    <TextView
        android:id="@+id/textViewBackToMenuConsultaCliente"
        android:layout_width="69dp"
        android:layout_height="35dp"
        android:layout_marginBottom="4dp"
        android:text="Voltar"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextConsultaEstadoCliente"
        android:layout_width="160dp"
        android:layout_height="40dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:background="@drawable/text_beauty_full"
        android:ems="10"
        android:hint="Estado"
        android:inputType="textPersonName"
        android:padding="6sp"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.452"
        app:layout_constraintStart_toEndOf="@+id/editTextNameClient"
        app:layout_constraintTop_toBottomOf="@+id/editTextConsultaEnderecoCliente"
        app:layout_constraintVertical_bias="0.027" />

    <Button
        android:id="@+id/btnRegisterConsultaCliente"
        android:layout_width="128dp"
        android:layout_height="47dp"
        android:background="@drawable/button_beauty_full"
        android:shadowColor="#000000"
        android:shadowDx="2"
        android:shadowDy="2"
        android:shadowRadius="1"
        android:text="Cadastrar"
        android:textSize="14sp"
        android:textStyle="bold"
        app:backgroundTint="#010127"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.308"
        app:layout_constraintStart_toEndOf="@+id/btnCancelConsultaCliente"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.847" />

    <EditText
        android:id="@+id/editTextConsultaNomeCliente"
        android:layout_width="160dp"
        android:layout_height="40dp"
        android:background="@drawable/text_beauty_full"
        android:ems="10"
        android:hint="Nome completo"
        android:inputType="textPersonName"
        android:padding="6sp"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.408" />
</androidx.constraintlayout.widget.ConstraintLayout>